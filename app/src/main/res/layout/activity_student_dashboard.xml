<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"


    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D2B4DE"

    tools:context="com.example.bhonesh.e_class.student_dashboard">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/image"
        android:layout_marginLeft="78dp"
        android:layout_marginStart="78dp"
        android:text="Name"


        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignEnd="@+id/textView14"
        android:layout_alignRight="@+id/textView14"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="40dp"
        android:text="Email"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="40dp"
        android:text="ID"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />



    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView12"
        android:layout_marginTop="40dp"
        android:text="Department"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />




    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView13"
        android:layout_marginTop="40dp"
        android:text="Batch"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"

        />



    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="40dp"
        android:text="Year"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/disname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView10"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />


    <TextView
        android:id="@+id/disemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:layout_below="@id/disname"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />


    <TextView
        android:id="@+id/disid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/disemail"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />



    <TextView
        android:id="@+id/disdepart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/disid"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />


    <TextView
        android:id="@+id/disbatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/disdepart"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />


    <TextView
        android:id="@+id/disyear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/disbatch"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />

    <ImageView
        android:id="@+id/image"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/disname"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/disname"
        android:layout_marginTop="36dp"
        app:srcCompat="@drawable/user" />

    <Button
        android:id="@+id/start_quiz"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"


        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:layout_toEndOf="@+id/mark"
        android:layout_toRightOf="@+id/mark"
        android:text="Quiz"
        android:textColor="#ffffff"
        android:background="#F5B7B1"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/mark"
        android:layout_width="100dp"
        android:layout_height="100dp"


        android:layout_alignTop="@+id/start_quiz"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_toLeftOf="@+id/disyear"
        android:layout_toStartOf="@+id/disyear"
        android:text="mark_attandance"
        android:textColor="#ffffff"
        android:background="#A3E4D7"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/download"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/start_quiz"
        android:layout_marginBottom="20dp"
        android:layout_toEndOf="@+id/start_quiz"
        android:layout_toRightOf="@+id/start_quiz"
        android:text="assign" />

    <Button
        android:id="@+id/chatroom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/disbatch"
        android:text="chatroom" />


</RelativeLayout>
