<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"


    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D2B4DE"
    tools:context="com.example.bhonesh.e_class.teacher_dashboard">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/image"
        android:layout_marginLeft="76dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="27dp"
        android:text="Name"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignBaseline="@+id/disemail"
        android:layout_alignBottom="@+id/disemail"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignStart="@+id/textView12"
        android:text="Email"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="40dp"
        android:text="ID"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />



    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView12"
        android:layout_marginTop="40dp"
        android:text="Department"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />




    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView13"
        android:layout_marginTop="40dp"
        android:text="Subject"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />



    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="40dp"
        android:text="Qualification"
        android:textSize="20sp"
        android:textColor="#000000"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/disname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView10"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />


    <TextView
        android:id="@+id/disemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/disname"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />


    <TextView
        android:id="@+id/disid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/disemail"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />



    <TextView
        android:id="@+id/disdepart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/disid"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />


    <TextView
        android:id="@+id/dissubject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/disdepart"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />


    <TextView
        android:id="@+id/disq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/dissubject"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:text="" />

    <ImageView
        android:id="@+id/image"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="23dp"
        android:layout_toEndOf="@+id/textView14"
        android:layout_toRightOf="@+id/textView14"
        app:srcCompat="@drawable/user" />

    <Button
        android:id="@+id/create_quiz"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/assignment"
        android:layout_toRightOf="@+id/assignment"
        android:background="#f57358"
        android:text="QUIZ"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <Button
        android:id="@+id/attand"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/assignment"
        android:layout_toStartOf="@+id/assignment"
        android:background="#F9E79F"
        android:text="Attandance"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <Button
        android:id="@+id/sub"
        android:layout_width="70dp"
        android:layout_height="70dp"


        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/attand"
        android:layout_toStartOf="@+id/attand"
        android:background="#F5B7B1"
        android:text="create subject"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <Button
        android:id="@+id/assignment"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="#b9a3e4"
        android:text="Assignment"

        android:textColor="#ffffff"
        android:textStyle="bold" />

    <Button
        android:id="@+id/chatroom"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/create_quiz"
        android:layout_toRightOf="@+id/create_quiz"
        android:background="#59ae3d"
        android:text="chatroom" />


</RelativeLayout>
